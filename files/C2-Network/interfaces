auto lo

iface lo inet loopback
#iface eth0 inet dhcp
auto eth0

allow-hotplug wlan0

auto wlan0
iface wlan0 inet static
  address 192.168.10.1
  netmask 255.255.255.0
    up     /sbin/iwconfig wlan0 mode TTTTTT && /sbin/iwconfig wlan0 enc restricted && /sbin/iwconfig wlan0 key [Y] XXXXXXXX && /sbin/iwconfig wlan0 essid SSSSSSSS

